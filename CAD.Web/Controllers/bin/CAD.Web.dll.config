    <membership defaultProvider="CADMembershipProvider">
      <providers>
        <clear />
        <add name="CADMembershipProvider" type="CAD.Web.Infraestrutura.Seguranca.Providers.CADMembershipProvider"
             enablePasswordRetrieval="false" enablePasswordReset="true" requiresQuestionAndAnswer="false" requiresUniqueEmail="false" maxInvalidPasswordAttempts="5" minRequiredPasswordLength="6" minRequiredNonalphanumericCharacters="0" passwordAttemptWindow="10" applicationName="CAD" passwordFormat="Hashed" connectionStringName="ConexaoCAD"/>
